{
  "name": "@newdash/newdash",
  "description": "javascript utility library",
  "version": "5.5.0",
  "license": "MIT",
  "main": "./dist/node/index",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newdash/newdash"
  },
  "contributors": [
    {
      "name": "Lodash Contributors",
      "url": "https://github.com/lodash/lodash"
    },
    {
      "name": "Theo Sun",
      "email": "theo.sun@outlook.com",
      "url": "https://github.com/Soontao"
    }
  ],
  "scripts": {
    "test": "jest",
    "test:deno": "deno test ./dist/deno/test/*.test.ts",
    "lint": "tsc --noEmit && eslint .",
    "coverage": "npm test -- --coverage --no-cache",
    "build:deno": "babel --out-dir dist/deno --extensions .js,.ts --keep-file-extension src",
    "build:node": "tsc --build tsconfig.json",
    "build:es6": "tsc --build tsconfig.es6.json",
    "build:es5": "tsc --build tsconfig.es5.json",
    "build:umd": "webpack",
    "build": "npm run build:node && npm run build:es5 && npm run build:es6 && npm run build:umd && npm run build:deno && npm run gen:package",
    "gen:package": "node scripts/copy-package-json.js",
    "prepublishOnly": "npm run build",
    "release": "standard-version --no-verify && git push --follow-tags origin master"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-typescript": "^7.10.1",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "standard-version": "^8.0.0",
    "terser-webpack-plugin": "^3.0.3",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "transform": {
      ".(js|ts|tsx)": "ts-jest"
    },
    "testTimeout": 30000,
    "collectCoverageFrom": [
      "src/**/*",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "src/test"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/deno/",
      "/dist/"
    ]
  },
  "keywords": [
    "javascript",
    "lodash",
    "utilities"
  ]
}
