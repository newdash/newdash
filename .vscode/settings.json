{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "cSpell.words": [
    "Deno",
    "Mutex",
    "falsey",
    "newdash",
    "nocheck"
  ],
  "javascript.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "editor.defaultFormatter": "vscode.typescript-language-features",
}
